<div id="product_{{ product_id }}" class="col-md-3 col-sm-6" data-hook="products_list_item" itemscope itemtype="http://schema.org/Product">
  <div class="panel panel-default">
    <div class="panel-body text-center product-body">
      {{ small_image_link }}
      <br/>
      {{ product_link }}
    </div>
    <div class="panel-footer text-center">
      <span class="price selling lead" itemprop="price">{{ product_price }}</span>
    </div>
  </div>
</div>
